<?xml version="1.0" encoding="UTF-8"?>
<types>
    <composite name="SpidersMessageHeader" description="Common fields of SPIDERS messages">
        <type name="channelRcvTimestampNs" primitiveType="int64"
            description="Timestamp of received message (optionally inserted by UDP transport)" />
        <type name="channelSndTimestampNs" primitiveType="int64"
            description="Timestamp of message sent (optionally inserted by UDP transport)" />
        <type name="TimestampNs" primitiveType="int64"
            description="Message Timestamp" />
        <type name="correlationId" primitiveType="int64"
            description="Unique number to track message" />
        <type name="tag" primitiveType="char" length="32"
            description="Tag field to identify message" />
    </composite>

    <type name="Dim" description="Non-zero unsigned" primitiveType="uint32" minValue="1" />
    <type name="Pad1" description="Pad 1 byte" primitiveType="int8" length="1" />
    <type name="Pad2" description="Pad 2 bytes" primitiveType="int8" length="2" />
    <type name="Pad3" description="Pad 3 bytes" primitiveType="int8" length="3" />
    <type name="Pad4" description="Pad 4 bytes" primitiveType="int8" length="4" />
    <type name="Int32Array2" description="Fixed length array (2) of int32" primitiveType="int32"
        length="2" />
    <type name="UInt32Array2" description="Fixed length array (2) of uint32" primitiveType="uint32"
        length="2" />
    <type name="Int32Array4" description="Fixed length array (4) of int32" primitiveType="int32"
        length="4" />
    <type name="UInt32Array4" description="Fixed length array (4) of uint32" primitiveType="uint32"
        length="4" />
    <type name="Int64Array2" description="Fixed length array (2) of int64" primitiveType="int64"
        length="2" />
    <type name="UInt64Array2" description="Fixed length array (4) of uint64" primitiveType="uint64"
        length="2" />
    <type name="Int64Array4" description="Fixed length array (2) of int64" primitiveType="int64"
        length="4" />
    <type name="UInt64Array4" description="Fixed length array (4) of uint64" primitiveType="uint64"
        length="4" />
</types>